<Window x:Class="WpfApp1.Master"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="Master" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="application" Margin="0,53,0,0" AutoGenerateColumns="False" Background="#FFF4E8D3" SelectedCellsChanged="application_SelectedCellsChanged" >
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding ID}"/>
                <DataGridTextColumn Header="IDUser" Binding="{Binding IDUser}"/>
                <DataGridTextColumn Header="Количество" Binding="{Binding Quantity}"/>
                <DataGridTextColumn Header="Cумма" Binding="{Binding Summ}"/>
                <DataGridTextColumn Header="Дата" Binding="{Binding Date}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
